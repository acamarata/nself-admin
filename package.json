{
  "name": "nself-admin",
  "version": "0.0.4",
  "private": false,
  "description": "Web-based administration interface for nself CLI backend stack",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "nself",
    "docker",
    "admin-panel",
    "dashboard",
    "monitoring",
    "container-management",
    "next-js",
    "react",
    "typescript",
    "self-hosted",
    "devops"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/acamarata/nself-admin.git"
  },
  "scripts": {
    "dev": "next dev --turbo -p ${PORT:-3021}",
    "build": "next build",
    "start": "next start -p ${PORT:-3021}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "docker:build": "docker build -t acamarata/nself-admin:latest -t acamarata/nself-admin:$(node -p \"require('./package.json').version\") .",
    "docker:push": "npm run docker:build && docker push acamarata/nself-admin:latest && docker push acamarata/nself-admin:$(node -p \"require('./package.json').version\")",
    "secure:setup": "node scripts/secure-setup.js",
    "password:hash": "node scripts/hash-password.js",
    "docker:release": "./scripts/docker-release.sh",
    "version:bump:patch": "npm version patch --no-git-tag-version",
    "version:bump:minor": "npm version minor --no-git-tag-version",
    "version:bump:major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm run docker:push"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@algolia/autocomplete-core": "^1.19.2",
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/dockerode": "^3.3.43",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/lokijs": "^1.5.14",
    "@types/mdx": "^2.0.13",
    "@types/pg": "^8.15.5",
    "@types/react-highlight-words": "^0.20.0",
    "acorn": "^8.15.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dockerode": "^4.0.7",
    "dotenv": "^17.2.1",
    "fast-glob": "^3.3.3",
    "flexsearch": "^0.8.205",
    "framer-motion": "^12.23.11",
    "js-yaml": "^4.1.0",
    "lightningcss": "^1.30.1",
    "lodash": "^4.17.21",
    "lokijs": "^1.5.12",
    "lucide-react": "^0.539.0",
    "mdast-util-to-string": "^4.0.0",
    "mdx-annotations": "^0.1.4",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-highlight-words": "^0.21.0",
    "recharts": "^3.1.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-mdx": "^3.1.0",
    "shiki": "^0.14.7",
    "simple-functional-loader": "^1.2.1",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "unist-util-filter": "^5.0.1",
    "unist-util-visit": "^5.0.0",
    "zod": "^4.1.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15",
    "eventsource": "^4.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "null-loader": "^4.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "0.34.3"
  }
}
